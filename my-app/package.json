  "author": "Yoshihide Shiono <shiono.yoshihide@gmail.com>",
  "main": "app/background.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:nextron\" \"npm run start:nest\"",
    "dev:nextron": "nextron --renderer-port 7776",
    "start:prod": "node dist/dist/main",
    "build": "npm run build:nest && npm run build:nextron",
    "build:nextron": "nextron build",
    "build:nest": "cd my-nest-app && nest build",
    "postinstall": "electron-builder install-app-deps",
    "start:nest": "cd my-nest-app && nest start --watch",
    "start": "nextron dev"
  },
